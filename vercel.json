{
  "routes": [
    {
      "src": "/static/(.*)", 
      "dest": "/static/$1"
    },
    {
      "src": "/assets/(.*)", 
      "dest": "/assets/$1"
    },
    {
      "src": "/statistic",
      "dest": "/index.html"
    },
    {
      "src": "/createArticle", 
      "dest": "/index.html"
    },
    {
      "src": "/updateArticle/:id", 
      "dest": "/index.html"
    },
    {
      "src": "/homePage", 
      "dest": "/index.html"
    },
    {
      "src": "/detailArticle/:id", 
      "dest": "/index.html"
    },
    {
      "src": "/category",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ]
}